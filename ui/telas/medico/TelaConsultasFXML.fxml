<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrpe.ui.controladores.telas.medico.TelaConsultasFXMLController">
   <children>
      <Label layoutX="312.0" layoutY="14.0" text="Consultas">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="tabelaConsultas" layoutX="6.0" layoutY="91.0" prefHeight="230.0" prefWidth="684.0">
        <columns>
          <TableColumn fx:id="dataCol" maxWidth="150.0" minWidth="110.0" prefWidth="120.0" text="Data" />
          <TableColumn fx:id="horaCol" maxWidth="150.0" minWidth="120.0" prefWidth="120.0" text="Hora" />
            <TableColumn fx:id="medicoCol" maxWidth="150.0" minWidth="120.0" prefWidth="120.0" text="Médico" />
            <TableColumn fx:id="descricaoCol" maxWidth="150.0" minWidth="120.0" prefWidth="120.0" text="Descrição" />
            <TableColumn fx:id="observacaoCol" maxWidth="150.0" minWidth="120.0" prefWidth="120.0" text="Observação" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="74.0" text="Lista de Consultas:" />
      <Button fx:id="observacoesB" layoutX="570.0" layoutY="346.0" mnemonicParsing="false" onAction="#setObservacoes" text="Inserir Observações" />
      <Button fx:id="voltarB" layoutX="497.0" layoutY="346.0" mnemonicParsing="false" onAction="#voltar" text="Voltar" />
   </children>
</AnchorPane>
